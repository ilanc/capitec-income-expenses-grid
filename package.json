{
  "name": "capitec-income-expenses-grid",
  "version": "1.0.0",
  "description": "Demo app for capitec test pilot",
  "main": "index.js",
  "scripts": {
    "build-public": "mkdir -p dist/ && rsync -rv --delete --delete-excluded --include-from  src/rsync-include.txt --exclude \\* src/ dist/",
    "build-ux-root": "webpack --progress --colors --display-modules --config src/webpack.config.js && cp src/index.html dist/",
    "build-ux-root-watch": "webpack --progress --colors --display-modules --config src/webpack.config.js --watch",
    "build": "npm-run-all build-public build-ux-root",
    "start-src": "live-server src/",
    "start-dist": "live-server dist/",
    "live-deploy": "surge dist capitec-test.cloudbank.co.za",
    "live": "npm-run-all build live-deploy"
  },
  "author": "Ilan Copelyn",
  "license": "MIT",
  "devDependencies": {
    "add-asset-html-webpack-plugin": "2.0.1",
    "aws-sdk": "2.100.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.1.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-stage-3": "6.24.1",
    "css-loader": "0.28.0",
    "ejs": "2.6.1",
    "ejs-loader": "0.3.0",
    "extend": "3.0.1",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "imports-loader": "0.7.1",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "morgan": "1.9.0",
    "node-fetch": "1.7.3",
    "npm-run-all": "3.1.2",
    "pdf.js-extract": "0.0.8",
    "riot": "3.8.1",
    "riot-compiler": "3.4.0",
    "riot-route": "3.1.2",
    "riot-tag-loader": "2.0.2",
    "script-loader": "0.7.0",
    "style-loader": "0.18.2",
    "surge": "0.19.0",
    "webpack": "3.4.1"
  }
}
